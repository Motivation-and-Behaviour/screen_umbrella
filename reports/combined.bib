  Journal = {Obesity Reviews},
  Number = {9},
  Pages = {833--849},
  Title = {An Evidence-Update on the Prospective Relationship between Childhood Sedentary Behaviour and Biomedical Health Indicators: A Systematic Review and Meta-Analysis},
